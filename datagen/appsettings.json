{
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "System": "Information",
        "Microsoft": "Information"
      },
      "Console":
      {
        "IncludeScopes": true
      },
      "ApplicationInsights": {
          "LogLevel": {
              "Default": "Information"
          }
      }
    },
    "ApplicationInsights": {
        "InstrumentationKey": "your-app-insights-instrumentation-key"
    },
    "CosmosDB_Endpoint": "https://your-cosmosdb-account.documents.azure.com:443/",
    "CosmosDB_KeyName": "your-cosmosdb-secret-name-in-keyvault",
    "CosmosDB_Database": "your-cosmosdb-database",
    "CosmosDB_Container": "your-cosmosdb-collection",
    "KeyVault_URL": "https://your-keyvault.vault.azure.net/"
  }